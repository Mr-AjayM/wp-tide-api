{
  "name": "wptide/wp-tide-api",
  "type": "wordpress-plugin",
  "description": "This is the WP Tide API plugin",
  "version": "1.0.0-beta",
  "homepage": "https://github.com/wptide/wp-tide-api/",
  "license": [ "GPL-2.0+" ],
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:wptide/wp-tide-api.git"
    }
  ],
  "authors": [
    {
      "name": "XWP",
      "homepage": "https://xwp.co"
    }
  ],
  "require": {
    "firebase/php-jwt": "^4.0",
    "aws/aws-sdk-php": "^3.22",
    "google/cloud-storage": "^1.4",
    "mongodb/mongodb": "^1.3"
  },
  "require-dev": {
    "wp-coding-standards/wpcs": "*",
    "wimg/php-compatibility": "*",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
    "php-coveralls/php-coveralls": "~2.1"
  },
  "config": {
    "platform": {
      "php": "7.2",
      "ext-redis": "3.2",
      "ext-mongodb": "1.40"
    }
  }
}
